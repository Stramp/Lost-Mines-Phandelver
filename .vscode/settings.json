{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "ms-vscode.cpptools",
  "[cpp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[c]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "files.autoSave": "off",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "C_Cpp.clang_format_path": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "C_Cpp.errorSquiggles": "disabled",
  "C_Cpp.intelliSenseEngine": "disabled",
  "editor.foldingStrategy": "indentation",
  "editor.showFoldingControls": "always",
  "C_Cpp.folding": "enabled",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Binaries": true,
    "**/Intermediate": true,
    "**/DerivedDataCache": true,
    "**/Saved": true,
    ".vscode/unreal-clangd": true
  },
  "search.exclude": {
    "**/Binaries": true,
    "**/Intermediate": true,
    "**/DerivedDataCache": true,
    "**/Saved": true,
    "**/.git": true
  },
  "files.watcherExclude": {
    "**/Binaries/**": true,
    "**/Intermediate/**": true,
    "**/DerivedDataCache/**": true,
    "**/Saved/**": true,
    "**/.vscode/unreal-clangd/**": true
  },
  "unreal-clangd.creation.overwrite": "partial",
  "unreal-clangd.utility.createProjectOnStartup": false,
  "unreal-clangd.utility.autoCreateProject": false,
  "unreal-clangd.utility.notifyOnNewFiles": false,
  "C_Cpp.autocomplete": "disabled",
  "C_Cpp.formatting": "disabled",
  "C_Cpp.updateChannel": "Default",
  "C_Cpp.configurationWarnings": "disabled",
  "C_Cpp.workspaceParsing": "disabled",
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": false,
  "C_Cpp.default.systemIncludePath": [],
  "unreal-clangd.creation.compilerPath": "c:\\Program Files\\LLVM\\bin\\clang-cl.exe",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "clangd.path": "c:\\Program Files\\LLVM\\bin\\clangd.exe",
  "clangd.arguments": [
    "-header-insertion=iwyu",
    "-header-insertion-decorators=true",
    "-all-scopes-completion=true",
    "-limit-results=100",
    "-background-index=true",
    "-limit-references=2000",
    "-completion-style=detailed",
    "-function-arg-placeholders=true",
    "-log=error"
  ],
  "clangd.detectExtensionConflicts": false,
  "clangd.onConfigChanged": "ignore",
  "clangd.checkUpdates": false,
  "files.associations": {
    "*.clangd": "yaml",
    "*.clang-format": "yaml",
    "*.clang-tidy": "yaml"
  },
  "workbench.colorCustomizations": {
    "editorInlayHint.foreground": "#a2a2a2c0",
    "editorInlayHint.background": "#00000000"
  },
  "editor.suggestFontSize": 0
}
